pipeline {

    agent any
        stage('Build') {
            steps {
                sh 'git clone https://github.com/MicrosoftDocs/pipelines-dotnet-core.git'
                sh 'sudo docker build -t aspnetapp .'
            }
        }
	  stage('Deploy') {
            steps {
                
                sh 'sudo docker run -d -p 8080:80 --name myapp aspnetapp'
            }
        }

    
}
