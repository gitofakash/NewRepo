pipeline {

    agent any
    stages{
        stage('Build') {
            steps {
		sh 'rm -rf *'
                sh 'git clone https://github.com/gitofakash/Dotnet.git'
                sh 'sudo docker build -t aspnetapp .'
            }
        }
	  stage('Deploy') {
            steps {
                
                sh 'sudo docker run -d -p 8080:80 --name myapp aspnetapp'
            }
        }

    }
}
